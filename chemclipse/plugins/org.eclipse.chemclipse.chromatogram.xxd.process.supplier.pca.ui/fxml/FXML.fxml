<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0"
	styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="org.eclipse.chemclipse.chromatogram.xxd.process.supplier.pca.ui.parts.FXMLController">
	<stylesheets>
		<URL value="@css/fxml.css" />
	</stylesheets>
	<children>
		<AreaChart fx:id="areaChart" title="TIC" layoutX="10"
			layoutY="10" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0"
			AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
			<xAxis>
				<NumberAxis side="BOTTOM" />
			</xAxis>
			<yAxis>
				<NumberAxis side="LEFT" />
			</yAxis>
		</AreaChart>
	</children>
</AnchorPane>
